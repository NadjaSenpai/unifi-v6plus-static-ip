# === UniFi OS (UDM / UDR) ===
WAN_IF=eth4
LAN_IF=br0
LAN_CIDR=192.168.1.0/24

# === v6plus static IPv4 via IPIP6 ===
# Static IPv4 (/32) - example (replace with your ISP-assigned address)
STATIC_V4=203.0.113.115

# Provider-assigned local IPv6 (prefix + IID) - example
# Add this to WAN as /128 and use it as the tunnel "local" address.
PROVIDER_ASSIGNED_LOCAL_V6=2001:db8:1234:5678::6f:6d45:7300:0

# BR IPv6 address (provided by ISP) - example
BR_V6=2001:db8:aaaa:bbbb::65

# === tunnel / MSS ===
TUN_IF=v6plus0
TUN_MTU=1452
MSS=1412

# === policy routing ===
ROUTE_TABLE=300
RULE_PREF=1000
